<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        tools:replace="android:icon"
        android:name="com.smile.fivecolorballs.FiveCBallsApp"
        android:icon="@drawable/fivecolorballs_app_icon"
        android:label="@string/fivecolorballs_app_name">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8354869049759576~8647116443" />

        <activity
            android:name="com.smile.fivecolorballs.MyActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.smile.fivecolorballs.SettingActivity"
            android:label="@string/settingStr"
            android:theme="@style/AppThemeTransparent" >
        </activity>

        <service
            android:name="com.smile.colorballs_main.services.LocalTop10Service"
            android:enabled="true"
            android:exported="false">
        </service>
        <service
            android:name="com.smile.colorballs_main.services.GlobalTop10Service"
            android:enabled="true"
            android:exported="false">
        </service>
    </application>

</manifest>
